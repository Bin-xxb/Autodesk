<?xml version="1.0" encoding="UTF-8"?>
<quilt layout="three-column" nestable="false" disableTopCssClass="false">
  <add to="common-footer">
    <component id="quilt:Footer"/>
  </add>
  <add to="mid-content">
    <component id="orientation"/>
  </add>
  <add to="common-header">
    <component id="quilt:Header"/>
    <component id="common.widget.core-node-icon" wrapper="lia-page-header"/>
    <component id="common.widget.page-title" wrapper="lia-page-header"/>
  </add>
  <add to="left-content">
    <component id="empty"/>
  </add>
  <remove from="site-navigation" for="Header">
    <component id="common.widget.site-navigation"/>
  </remove>
</quilt>