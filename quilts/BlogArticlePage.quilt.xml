    <component id="quilt:Footer"/>
  </add>
  <add to="main-content">
    <component id="menu-bar-without-options"/>
    <component id="article"/>
    <component id="custom.autodesk.blog-topic-message-bio"/>
    <component id="comment-count-conditional"/>
    <component id="comment-list"/>
    <component id="comment-pager"/>
    <component id="comments-awaiting-moderation"/>
    <component id="comment-editor"/>
    <component id="autodesk-custom-blog-share" wrapper="content-navigator-share"/>
    <component id="article-navigator" wrapper="content-navigator-share"/>
    <component id="autodesk-custom-blog-article-recommended"/>
    <component id="custom.autodesk.blog-subscribe"/>
  </add>
  <add to="common-header">
    <component id="quilt:Header"/>
    <component id="autodesk-custom-banner"/>
    <component id="article-navigator" wrapper="header-navigator-share"/>
